{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "builds": [
      { "src": "src/server.js", "use": "@vercel/node" },
      { "src": "src/**/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/services/(.*)", "dest": "/src/server.js" },
      { "src": "/waitlist/(.*)", "dest": "/src/server.js" },
      { "src": "/(.*)", "dest": "/src/$1" }
    ]
  }